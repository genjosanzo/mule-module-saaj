<mule xmlns="http://www.mulesource.org/schema/mule/core/2.2"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns:spring="http://www.springframework.org/schema/beans"
      xmlns:context="http://www.springframework.org/schema/context"
      xmlns:jms="http://www.mulesource.org/schema/mule/jms/2.2"
      xmlns:http="http://www.mulesource.org/schema/mule/http/2.2"
      xmlns:script="http://www.mulesource.org/schema/mule/scripting/2.2"
      xmlns:xm="http://www.mulesource.org/schema/mule/xml/2.2"
      xmlns:saaj="http://www.mulesource.org/schema/mule/saaj/2.2"
      xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd
       http://www.mulesource.org/schema/mule/core/2.2 http://www.mulesource.org/schema/mule/core/2.2/mule.xsd
       http://www.mulesource.org/schema/mule/jms/2.2 http://www.mulesource.org/schema/mule/jms/2.2/mule-jms.xsd
       http://www.mulesource.org/schema/mule/http/2.2 http://www.mulesource.org/schema/mule/http/2.2/mule-http.xsd
       http://www.mulesource.org/schema/mule/scripting/2.2 http://www.mulesource.org/schema/mule/scripting/2.2/mule-scripting.xsd
       http://www.mulesource.org/schema/mule/xml/2.2 http://www.mulesource.org/schema/mule/xml/2.2/mule-xml.xsd
       http://www.mulesource.org/schema/mule/saaj/2.2 http://www.mulesource.org/schema/mule/saaj/2.2/mule-saaj.xsd
       "
        >

    <jms:activemq-connector name="jmsConnector" brokerURL="vm://localhost"/>

    <model name="saajReceiverModel">

        <service name="saajProxy">
            <inbound>
                <http:inbound-endpoint address="http://localhost:9756/soap">
                    <transformers>
                        <saaj:soap-message-to-document-transformer/>
                        <xm:dom-to-xml-transformer/>
                    </transformers>
                    <response-transformers>
                                                
                    </response-transformers>
                </http:inbound-endpoint>
            </inbound>

        </service>
    </model>


</mule>